<h1>Edit the Review</h1>
<%# song#new %>
<<%= form_for @song do |f|%>
    Artist Name: <%= f.text_field :artist_name %> <br>
    Song Name: <%= f.text_field :title %> <br>
    Genre: <%= f.text_field :genre %> <br>
    Language: <%= f.text_field :language %> <br>
    URL: <%= f.text_field :link %>
    <br>
    <%= f.fields_for :review do |r| %>
    Song Review: <%= r.text_area :review %>
    <% end %>
    <br>
    <%= f.submit %>
<% end %> >


<%#= render partial: "form", locals: (song: @song) %>