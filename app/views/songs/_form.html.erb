<%= form_for @song do |f|%>
<p>
    Artist Name: <%= f.text_field :artist_name %> 
</p>
<p>
    Song Name: <%= f.text_field :title %>
</p>
<p>
    Genre: <%= f.text_field :genre %>
</p>
<p>
    Language: <%= f.text_field :language %>
</p>
<p>
    URL: <%= f.text_field :link %>
</p>
<p>
    <%= f.fields_for :review do |r| %>
    Song Review: <%= r.text_area :review %>
    <% end %>
</p>
    <%= f.submit %>
<% end %> 